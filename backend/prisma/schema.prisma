generator db {
  provider = "go run github.com/prisma/prisma-client-go"
}

datasource db {
  provider = "sqlite"
  url      = "file:pad.db"
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model admins {
  id       String? @id
  login    String
  password String

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model categories {
  id            String?         @id
  name          String
  image_url     String
  product_types product_types[]

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model customer_orders {
  id          String?  @id
  amount      Decimal
  created_at  DateTime
  customer_id Int

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model customers {
  id          String?  @id
  name        String
  email       String
  phone       String
  address     String
  city_region String
  cc_number   String
  orders      orders[]

  @@ignore
}

model ordered_products {
  product_id        String
  customer_order_id String
  orders            orders   @relation(fields: [customer_order_id], references: [id]) @ignore
  products          products @relation(fields: [product_id], references: [id]) @ignore

  @@id([product_id, customer_order_id])
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model orders {
  id               String?            @id
  amount           Decimal
  created_at       DateTime
  customer_id      String
  customers        customers          @relation(fields: [customer_id], references: [id])
  ordered_products ordered_products[]

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model product_types {
  id          String?    @id
  name        String
  category_id String
  categories  categories @relation(fields: [category_id], references: [id])
  products    products[]

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model products {
  id               String?            @id
  name             String
  price            Decimal
  description      String
  delivery_date    DateTime
  image_url        String
  product_type_id  String
  product_types    product_types      @relation(fields: [product_type_id], references: [id])
  ordered_products ordered_products[]

  @@ignore
}
